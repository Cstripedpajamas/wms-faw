<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.base.packageInfo.mapper.WmsPackinfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.base.packageInfo.entity.WmsPackinfo">
        <id column="id" property="id" />
        <result column="packge_name" property="packgeName" />
        <result column="packge_specif" property="packgeSpecif" />
        <result column="mark" property="mark" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS "id", packge_name AS "packgeName", packge_specif AS "packgeSpecif", mark AS "mark", create_time AS "createTime"
    </sql>


    <select id="customList" resultType="cn.stylefeng.guns.modular.base.packageInfo.model.result.WmsPackinfoResult" parameterType="cn.stylefeng.guns.modular.base.packageInfo.model.params.WmsPackinfoParam">
        select
        <include refid="Base_Column_List"/>
        from wms_packinfo where 1 = 1
            <if test="paramCondition.packgeName != null and paramCondition.packgeName != ''">
                and packge_name like CONCAT('%',#{paramCondition.packgeName},'%')
            </if>
            <if test="paramCondition.packgeSpecif != null and paramCondition.packgeSpecif != ''">
                and packge_specif like CONCAT('%',#{paramCondition.packgeSpecif},'%')
            </if>
    </select>

    <select id="customMapList" resultType="map" parameterType="cn.stylefeng.guns.modular.base.packageInfo.model.params.WmsPackinfoParam">
        select
        <include refid="Base_Column_List"/>
        from wms_packinfo where 1 = 1
            <if test="paramCondition.packgeName != null and paramCondition.packgeName != ''">
                and packge_name like CONCAT('%',#{paramCondition.packgeName},'%')
            </if>
            <if test="paramCondition.packgeSpecif != null and paramCondition.packgeSpecif != ''">
                and packge_specif like CONCAT('%',#{paramCondition.packgeSpecif},'%')
            </if>
    </select>

    <select id="customPageList" resultType="cn.stylefeng.guns.modular.base.packageInfo.model.result.WmsPackinfoResult" parameterType="cn.stylefeng.guns.modular.base.packageInfo.model.params.WmsPackinfoParam">
        select
        <include refid="Base_Column_List"/>
        from wms_packinfo where 1 = 1
            <if test="paramCondition.packgeName != null and paramCondition.packgeName != ''">
                and packge_name like CONCAT('%',#{paramCondition.packgeName},'%')
            </if>
            <if test="paramCondition.packgeSpecif != null and paramCondition.packgeSpecif != ''">
                and packge_specif like CONCAT('%',#{paramCondition.packgeSpecif},'%')
            </if>
    </select>

    <select id="customPageMapList" resultType="map" parameterType="cn.stylefeng.guns.modular.base.packageInfo.model.params.WmsPackinfoParam">
        select
        <include refid="Base_Column_List"/>
        from wms_packinfo where 1 = 1
            <if test="paramCondition.packgeName != null and paramCondition.packgeName != ''">
                and packge_name like CONCAT('%',#{paramCondition.packgeName},'%')
            </if>
            <if test="paramCondition.packgeSpecif != null and paramCondition.packgeSpecif != ''">
                and packge_specif like CONCAT('%',#{paramCondition.packgeSpecif},'%')
            </if>
    </select>

    <select id="findByMaterialTypeId" parameterType="string" resultType="cn.stylefeng.guns.modular.base.packageInfo.entity.WmsPackinfo">
        SELECT
        wp.*
        FROM wms_packinfo AS wp
        JOIN wms_material_type AS wmt
        ON wp.id = wmt.package_type AND wmt.id = #{id}
    </select>

</mapper>
