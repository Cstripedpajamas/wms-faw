    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.fawPurchase0rder.entity.FawmallPurchaseorderCancel">
        <id column="id" property="id" />
        <result column="purdocno" property="purdocno" />
        <result column="itemno" property="itemno" />
        <result column="STATUS" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS "id", purdocno AS "purdocno", itemno AS "itemno", STATUS AS "status"
    </sql>


