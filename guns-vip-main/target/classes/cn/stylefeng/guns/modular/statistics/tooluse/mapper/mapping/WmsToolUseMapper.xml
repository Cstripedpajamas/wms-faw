    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.statistics.tooluse.entity.WmsToolUse">
        <id column="id" property="id" />
        <result column="operator" property="operator" />
        <result column="material_type_id" property="materialTypeId" />
        <result column="material_name" property="materialName" />
        <result column="material_sku" property="materialSku" />
        <result column="material_id" property="materialId" />
        <result column="material_serial_number" property="materialSerialNumber" />
        <result column="data_time" property="dataTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS "id", operator AS "operator", material_type_id AS "materialTypeId", material_name AS "materialName", material_sku AS "materialSku", material_id AS "materialId", material_serial_number AS "materialSerialNumber", data_time AS "dataTime"
