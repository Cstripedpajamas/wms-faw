    <select id="customPageMapList" resultType="map" parameterType="cn.stylefeng.guns.modular.statistics.equipmentfailurestatistics.model.params.WmsEquipmentFailureStatisticsParam">
        select
        <include refid="Base_Column_List"/>
        from wms_equipment_failure_statistics where 1 = 1
            <if test="paramCondition.id != null and paramCondition.id != ''">
                and id like CONCAT('%',#{paramCondition.id},'%')
            </if>
            <if test="paramCondition.equipmentName != null and paramCondition.equipmentName != ''">
                and equipment_name like CONCAT('%',#{paramCondition.equipmentName},'%')
            </if>
            <if test="paramCondition.failureType != null and paramCondition.failureType != ''">
                and failure_type like CONCAT('%',#{paramCondition.failureType},'%')
            </if>
            <if test="paramCondition.failureDescribe != null and paramCondition.failureDescribe != ''">
                and failure_describe like CONCAT('%',#{paramCondition.failureDescribe},'%')
            </if>
            <if test="paramCondition.createTime != null and paramCondition.createTime != ''">
                and create_time like CONCAT('%',#{paramCondition.createTime},'%')
            </if>
    </select>


    <select id="customPageList" resultType="map" parameterType="cn.stylefeng.guns.modular.statistics.equipmentfailurestatistics.model.params.WmsEquipmentFailureStatisticsParam">
        select wmt.material_type as materialType,wmt.material_name as
            materialName,wmt.material_sku as materialSku,wmt.m_unit as
                mUnit,wmt.sizes as Sizes,COUNT(1) as repairCount from
            wms_cabinet1_repair_task as wcr LEFT JOIN wms_material_type as
                wmt ON wcr.material_type_id=wmt.id where 1 = 1
        <if test="paramCondition.materialName != null and paramCondition.materialName != ''">
            and wmt.material_name like CONCAT('%',#{paramCondition.materialName},'%')
        </if>
        <if test="paramCondition.materialSku != null and paramCondition.materialSku != ''">
            and wmt.material_sku like CONCAT('%',#{paramCondition.materialSku},'%')
        </if>
        <if test="paramCondition.Sizes != null and paramCondition.Sizes != ''">
            and wmt.sizes like CONCAT('%',#{paramCondition.Sizes},'%')
        </if>
<!--        <if test="paramCondition.BeginTime != null and paramCondition.BeginTime != '' and paramCondition.EndTime != null and paramCondition.EndTime != ''">-->
<!--            and wcr.create_time between #{paramCondition.BeginTime} and #{paramCondition.EndTime}-->
<!--        </if>-->
        GROUP BY wcr.material_type_id
    </select>

