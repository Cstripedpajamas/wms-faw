<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.statistics.equipmentfailurestatistics.mapper.WmsEquipmentFailureStatisticsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.statistics.equipmentfailurestatistics.entity.WmsEquipmentFailureStatistics">
        <id column="id" property="id" />
        <result column="equipment_name" property="equipmentName" />
        <result column="failure_type" property="failureType" />
        <result column="failure_describe" property="failureDescribe" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS "id", equipment_name AS "equipmentName", failure_type AS "failureType", failure_describe AS "failureDescribe", create_time AS "createTime"
    </sql>


    <select id="customList" resultType="cn.stylefeng.guns.modular.statistics.equipmentfailurestatistics.model.result.WmsEquipmentFailureStatisticsResult" parameterType="cn.stylefeng.guns.modular.statistics.equipmentfailurestatistics.model.params.WmsEquipmentFailureStatisticsParam">
        select
        <include refid="Base_Column_List"/>
        from wms_equipment_failure_statistics where 1 = 1
            <if test="paramCondition.id != null and paramCondition.id != ''">
                and id like CONCAT('%',#{paramCondition.id},'%')
            </if>
            <if test="paramCondition.equipmentName != null and paramCondition.equipmentName != ''">
                and equipment_name like CONCAT('%',#{paramCondition.equipmentName},'%')
            </if>
            <if test="paramCondition.failureType != null and paramCondition.failureType != ''">
                and failure_type like CONCAT('%',#{paramCondition.failureType},'%')
            </if>
            <if test="paramCondition.failureDescribe != null and paramCondition.failureDescribe != ''">
                and failure_describe like CONCAT('%',#{paramCondition.failureDescribe},'%')
            </if>
            <if test="paramCondition.createTime != null and paramCondition.createTime != ''">
                and create_time like CONCAT('%',#{paramCondition.createTime},'%')
            </if>
    </select>

    <select id="customMapList" resultType="map" parameterType="cn.stylefeng.guns.modular.statistics.equipmentfailurestatistics.model.params.WmsEquipmentFailureStatisticsParam">
        select
        <include refid="Base_Column_List"/>
        from wms_equipment_failure_statistics where 1 = 1
            <if test="paramCondition.id != null and paramCondition.id != ''">
