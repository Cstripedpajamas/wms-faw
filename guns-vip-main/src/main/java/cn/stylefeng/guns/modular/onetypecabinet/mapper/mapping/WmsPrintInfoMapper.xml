    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.onetypecabinet.entity.WmsPrintInfo">
        <id column="id" property="id" />
        <result column="material_serial_number" property="materialSerialNumber" />
        <result column="operator" property="operator" />
        <result column="print_type" property="printType" />
        <result column="print_stu" property="printStu" />
        <result column="create_time" property="createTime" />
        <result column="data_time" property="dataTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS "id", material_serial_number AS "materialSerialNumber", operator AS "operator", print_type AS "printType", print_stu AS "printStu", create_time AS "createTime", data_time AS "dataTime"
    </sql>


    <select id="customList" resultType="cn.stylefeng.guns.modular.onetypecabinet.model.result.WmsPrintInfoResult" parameterType="cn.stylefeng.guns.modular.onetypecabinet.model.params.WmsPrintInfoParam">
        select
        <include refid="Base_Column_List"/>
        from wms_print_info where 1 = 1
            <if test="paramCondition.id != null and paramCondition.id != ''">
                and id like CONCAT('%',#{paramCondition.id},'%')
            </if>
            <if test="paramCondition.materialSerialNumber != null and paramCondition.materialSerialNumber != ''">
                and material_serial_number like CONCAT('%',#{paramCondition.materialSerialNumber},'%')
            </if>
            <if test="paramCondition.operator != null and paramCondition.operator != ''">
