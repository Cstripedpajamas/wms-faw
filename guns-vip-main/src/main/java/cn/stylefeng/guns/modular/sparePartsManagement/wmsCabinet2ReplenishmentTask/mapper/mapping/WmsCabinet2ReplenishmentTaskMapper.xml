    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS "id", task_number AS "taskNumber", material_type_id AS "materialTypeId", material_id AS "materialId", m_batch AS "mBatch", m_number AS "mNumber", stock_id AS "stockId",turnover_id AS "turnoverId",turnover_number AS "turnoverNumber", loca_number AS "locaNumber", operator AS "operator", operation_time AS "operationTime", task_state AS "taskState", create_time AS "createTime", update_time AS "updateTime"
